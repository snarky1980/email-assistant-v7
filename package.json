{
  "name": "email-assistant-v7-local",
  "version": "1.0.0",
  "description": "Local dev server for Email Assistant with OpenAI proxy",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "set-admin-token": "node scripts/set-admin-token.js",
    "doctor": "node scripts/doctor.js",
    "lint": "eslint . --ext .js",
    "test": "jest --runInBand --passWithNoTests",
    "build-marker": "node scripts/update-build-marker.js",
    "prepare": "npm run build-marker || true",
    "version-endpoint": "node -e \"require('./server').start();setTimeout(async()=>{const r=await fetch('http://localhost:'+process.env.PORT||3000+'/api/version');console.log(await r.text());process.exit(0);},1200);\"",
    "verify-popup-version": "node scripts/verify-popup-version.js"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^2.6.9"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4"
  }
}
